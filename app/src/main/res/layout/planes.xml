<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/ImgHogarPesas"
        android:layout_width="139dp"
        android:layout_height="131dp"
        android:src="@drawable/hogarpesas"
        tools:layout_editor_absoluteX="136dp"
        tools:layout_editor_absoluteY="456dp" />

    <ImageView
        android:id="@+id/ImgHogar"
        android:layout_width="133dp"
        android:layout_height="145dp"
        android:src="@drawable/hogar"
        tools:layout_editor_absoluteX="139dp"
        tools:layout_editor_absoluteY="244dp" />

    <TextView
        android:id="@+id/TextHogar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Hogar"
        android:textSize="20sp"
        tools:layout_editor_absoluteX="178dp"
        tools:layout_editor_absoluteY="398dp" />

    <TextView
        android:id="@+id/TextHogarPesas"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Hogar con pesas"
        android:textSize="16sp"
        tools:layout_editor_absoluteX="145dp"
        tools:layout_editor_absoluteY="604dp" />

    <TextView
        android:id="@+id/TextSeleccione"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#211F1F"
        android:text="SELECCIONE"
        android:textColor="#E7E4E4"
        android:textColorLink="#EDE3E3"
        android:textSize="48sp"
        tools:layout_editor_absoluteX="75dp"
        tools:layout_editor_absoluteY="116dp" />
</androidx.constraintlayout.widget.ConstraintLayout>